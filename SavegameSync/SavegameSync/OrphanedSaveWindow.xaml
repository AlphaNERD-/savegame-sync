<Window x:Class="SavegameSync.OrphanedSaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SavegameSync"
        mc:Ignorable="d"
        Title="OrphanedSaveWindow" Height="317.647" Width="300">
    <Grid>
        <ListBox x:Name="orphanedSaveListBox" HorizontalAlignment="Left" Height="169" Margin="33,44,0,0" VerticalAlignment="Top" Width="223"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="33,23,0,0" TextWrapping="Wrap" Text="Orphaned saves" VerticalAlignment="Top" Width="93"/>
        <Button x:Name="downloadSaveButton" Content="Download save" HorizontalAlignment="Left" Margin="33,218,0,0" VerticalAlignment="Top" Width="93" Click="downloadSaveButton_Click"/>
        <Button x:Name="deleteSaveButton" Content="Delete save" HorizontalAlignment="Left" Margin="33,243,0,0" VerticalAlignment="Top" Width="93" Click="deleteSaveButton_Click"/>
        <Button x:Name="backButton" Content="Back" HorizontalAlignment="Left" Margin="181,19,0,0" VerticalAlignment="Top" Width="75" Click="backButton_Click"/>

    </Grid>
</Window>
